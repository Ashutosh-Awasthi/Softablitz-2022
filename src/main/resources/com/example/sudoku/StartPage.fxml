      <Button layoutX="83.0" layoutY="23.0" mnemonicParsing="false" onAction="#onSigninButtonClick" text="Sign in">
         <graphic>
            <FontAwesomeIconView glyphName="SIGN_IN" />
         </graphic>
      </Button>
      <Label layoutX="176.0" layoutY="148.0" text="Mode">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="188.0" layoutY="183.0" text="Size">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="consoleLabel" layoutX="14.0" layoutY="369.0" prefHeight="18.0" prefWidth="574.0" />
   </children>
</AnchorPane>
